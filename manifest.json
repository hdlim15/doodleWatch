{
	// Required
	"manifest_version": 2,

	"name": "doodleWatch",
	"version": "0.0.1",
	"author": "Hyuckin David Lim",

	// Recommended
	// "default_locale": "en",
	"description": "An extension to keep your eyes from getting bored",
	"icons": {
		"16": "images/temp_icon16.png",
		"128": "images/temp_icon128.png"
	},

	"browser_action": {
		"default_icon": "images/temp_icon32.png",
		"default_title": "lit animations",
		"default_popup": "popup.html"
	},

	"background": {
		// scripts are contained in background.html
		"page": "background.html",
		"persistent": false
	}



}